generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas   = ["auth", "public"]
}

model UserProfile {
  id            String         @id @map("id") @db.Uuid
  email         String         @unique
  name          String?
  avatarUrl     String?        @map("avatar_url")
  createdAt     DateTime       @default(now()) @map("created_at")
  updatedAt              DateTime       @updatedAt @map("updated_at")
  provider               String?        @default("email")
  userType               String?        @default("general") @map("user_type")
  hasCompletedOnboarding Boolean?       @default(false) @map("has_completed_onboarding")
  childProfiles          ChildProfile[]

  @@map("user_profiles")
  @@schema("public")
}

model ChildProfile {
  id        String      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId    String      @map("user_id") @db.Uuid
  name      String
  birthYear Int         @map("birth_year")
  emoji     String?     @default("ðŸ¥")
  createdAt DateTime    @default(now()) @map("created_at")
  updatedAt DateTime    @updatedAt @map("updated_at")
  ageGroup  String?     @map("age_group")
  user      UserProfile @relation(fields: [userId], references: [id], onDelete: Cascade)
  readingRecords reading_records[]

  @@index([userId])
  @@map("child_profiles")
  @@schema("public")
}

model Bookshelf {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId     String    @map("user_id") @db.Uuid
  isbn       String
  title      String
  author     String?
  bookImage  String?   @map("book_image")
  status     String?   @default("want_to_read")
  addedAt    DateTime? @default(now()) @map("added_at") @db.Timestamptz(6)
  finishedAt DateTime? @map("finished_at") @db.Timestamptz(6)
  domain     String?
  user       users     @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([userId, isbn])
  @@index([addedAt(sort: Desc)], map: "idx_bookshelf_added_at")
  @@index([status], map: "idx_bookshelf_status")
  @@index([userId], map: "idx_bookshelf_user_id")
  @@map("bookshelf")
  @@schema("public")
}

model Collection {
  id                    String           @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title                 String
  publisher             String
  targetAgeMonthsStart  Int?             @map("target_age_months_start")
  target_age_months_end Int?
  total_count           Int?
  category              String?
  features              Json?            @default("{}")
  summary               String?
  pros                  String[]
  cons                  String[]
  created_at            DateTime?        @default(now()) @db.Timestamptz(6)
  updated_at            DateTime?        @default(now()) @db.Timestamptz(6)
  blogReviewCount       Int              @default(0) @map("blog_review_count")
  rankingUpdatedAt      DateTime?        @map("ranking_updated_at")
  salesIndex            Int              @default(0) @map("sales_index")
  verified_books        verified_books[]

  @@unique([publisher, title])
  @@index([publisher], map: "idx_collections_publisher")
  @@map("collections")
  @@schema("public")
}

